server:
  <% @server_options.each do |k, v| %>
  <%= k %>: <%= v %>
  <% end %>

remote-control:
  control-enable: yes
<% @config_categories.each do |config_category, configs| %>
<% if configs.is_a?(Array) %>
<% configs.each do |config| %>

<%= config_category %>:
  <% config.each do |k, v| %>
  <%= k %>: <%= v %>
  <% end %>
<% end %>
<% end %>
<% end %>
